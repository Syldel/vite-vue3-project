{
  "prettier.enable": false,
  "eslint.enable": true,
  "eslint.packageManager": "yarn",
  "editor.formatOnSave": false,
  "eslint.format.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "vue",
    "yml",
    "yaml"
  ],
  "css.customData": [".vscode/tailwind.json"]
}
